{
  "compilerOptions": {
    "target": "es6",                    
    "module": "commonjs",               
    "lib": ["dom","es2018"],            
    "sourceMap": true, 
    "rootDir": "./",                    
    "outDir": "dist",   
    "strict": true,                     
    "noImplicitAny": true,              
     "noUnusedLocals": true,            
    "noImplicitReturns": true,          
    "noFallthroughCasesInSwitch": true, 
    "moduleResolution": "node",         
    "esModuleInterop": true,
  }
}