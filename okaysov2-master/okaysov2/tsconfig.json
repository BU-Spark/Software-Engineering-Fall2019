{
  "compilerOptions": {
    "module": "es6",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "target": "es6",
    "lib": [
      "es2016",
      "esnext.asynciterable"
    ],
    "jsx": "react-native",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "types": [
      "jest"
    ]
  }
}